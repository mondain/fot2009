<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<contextName>appserver261</contextName>
	
	<jmxConfigurator contextName="appserver261" />

	<appender name="APPSERVER261" class="ch.qos.logback.core.FileAppender">
		<File>log/appserver261.log</File>
		<Append>false</Append>
		<Encoding>UTF-8</Encoding>
		<BufferedIO>false</BufferedIO>
		<ImmediateFlush>true</ImmediateFlush>
		<layout class="ch.qos.logback.classic.PatternLayout">
			<Pattern>
				%date [%thread] %-5level %logger{35} - %msg%n
			</Pattern>
		</layout>
	</appender>
	<root>
		<appender-ref ref="APPSERVER261" />
	</root>		
</configuration>
